{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"shared-store": ["../shared-store"],
			"shared-store/*": ["../shared-store/*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/+not-found.jsx",
		"app/index.jsx",
		"app/(tabs)/index.jsx",
		"app/(tabs)/explore.jsx",
		"app/(tabs)/index.jsx"
	]
}
